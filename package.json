{
  "name": "prettier_d",
  "version": "1.0.0",
  "description": "Makes prettier fast",
  "bin": "./lib/bin/prettier_d.js",
  "keywords": [
    "prettier",
    "daemon"
  ],
  "author": "Mike Wyatt <wyatt.mike@gmail.com>",
  "contributors": [],
  "homepage": "https://github.com/mikew/prettier_d",
  "scripts": {
    "lint": "eslint . --ignore-pattern '**/node_modules/**'",
